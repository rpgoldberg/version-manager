{
  "application": {
    "name": "figure-collector-services",
    "version": "1.0.0",
    "releaseDate": "09-Aug-2025",
    "description": "Figure collection management application"
  },
  "services": {
    "backend": {
      "name": "figure-collector-backend",
      "version": "1.0.0",
      "repository": "figure-collector-backend",
      "dockerImage": "backend:v1.0.0",
      "ownedBy": "figure-collector",
      "coupledWith": [
        "frontend"
      ]
    },
    "frontend": {
      "name": "figure-collector-frontend",
      "version": "1.0.0",
      "repository": "figure-collector-frontend",
      "dockerImage": "frontend:v1.0.0",
      "ownedBy": "figure-collector",
      "coupledWith": [
        "backend"
      ]
    },
    "scraper": {
      "name": "page-scraper",
      "version": "1.0.0",
      "repository": "page-scraper",
      "dockerImage": "scraper:v1.0.0",
      "ownedBy": "platform",
      "coupledWith": [],
      "standalone": true,
      "description": "Generic web scraping service"
    },
    "infrastructure": {
      "name": "figure-collector-infra",
      "version": "1.0.0",
      "repository": "figure-collector-infra",
      "ownedBy": "figure-collector",
      "dockerImage": "infrastructure:v1.0.0"
    }
  },
  "dependencies": {
    "backend": {
      "scraper": "^1.0.0"
    },
    "frontend": {
      "backend": "^1.0.0"
    }
  },
  "compatibility": {
    "testedCombinations": [
      {
        "backend": "1.0.0",
        "frontend": "1.0.0",
        "scraper": "1.0.0",
        "verified": "09-Aug-2025"
      }
    ]
  }
}
