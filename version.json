{
  "application": {
    "name": "figure-collector-services",
    "version": "2.0.2",
    "releaseDate": "30-Oct-2025",
    "description": "Figure collection management application"
  },
  "services": {
    "backend": {
      "name": "figure-collector-backend",
      "version": "2.0.2",
      "repository": "figure-collector-backend",
      "dockerImage": "backend:v2.0.2",
      "ownedBy": "figure-collector",
      "coupledWith": [
        "frontend"
      ]
    },
    "frontend": {
      "name": "figure-collector-frontend",
      "version": "2.0.2",
      "repository": "figure-collector-frontend",
      "dockerImage": "frontend:v2.0.2",
      "ownedBy": "figure-collector",
      "coupledWith": [
        "backend"
      ]
    },
    "scraper": {
      "name": "page-scraper",
      "version": "2.0.3",
      "repository": "page-scraper",
      "dockerImage": "scraper:v2.0.3",
      "ownedBy": "platform",
      "coupledWith": [],
      "standalone": true,
      "description": "Generic web scraping service"
    },
    "infrastructure": {
      "name": "figure-collector-infra",
      "version": "2.0.2",
      "repository": "figure-collector-infra",
      "ownedBy": "figure-collector",
      "dockerImage": "infrastructure:v2.0.2"
    },
    "version-manager": {
      "name": "version-manager",
      "version": "1.1.3",
      "repository": "version-manager",
      "dockerImage": "version-manager:v1.1.3",
      "ownedBy": "platform",
      "coupledWith": []
    },
    "integration_tests": {
      "name": "integration_tests",
      "version": "1.0.2",
      "repository": "integration_tests",
      "dockerImage": "integration_tests:v1.0.2",
      "ownedBy": "platform",
      "coupledWith": []
    }
  },
  "dependencies": {
    "backend": {
      "scraper": "^2.0.3"
    },
    "frontend": {
      "backend": "^2.0.2"
    }
  },
  "compatibility": {
    "testedCombinations": [
      {
        "backend": "1.0.0",
        "frontend": "1.0.0",
        "scraper": "1.0.0",
        "verified": "09-Aug-2025"
      },
      {
        "backend": "1.1.0",
        "frontend": "1.1.0",
        "scraper": "1.1.0",
        "verified": "01-Sep-2025"
      },
      {
        "backend": "2.0.0",
        "frontend": "2.0.0",
        "scraper": "2.0.0",
        "verified": "26-Oct-2025"
      },
      {
        "backend": "2.0.1",
        "frontend": "2.0.1",
        "scraper": "2.0.1",
        "verified": "26-Oct-2025"
      },
      {
        "backend": "2.0.2",
        "frontend": "2.0.2",
        "scraper": "2.0.2",
        "version-manager": "1.1.2",
        "integration-tests": "1.0.2",
        "verified": "30-Oct-2025",
        "notes": "Simplified versioning strategy - removed dual-tagging"
      },
      {
        "backend": "2.0.2",
        "frontend": "2.0.2",
        "scraper": "2.0.3",
        "version-manager": "1.1.3",
        "integration-tests": "1.0.2",
        "verified": "09-Nov-2025",
        "notes": "Incremental release: page-scraper performance improvements (6x faster with browser context reuse, NSFW auth, Chrome CVE fixes)"
      }
    ]
  }
}
